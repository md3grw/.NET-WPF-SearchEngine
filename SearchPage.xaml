      d:DesignHeight="1080" d:DesignWidth="1920" 
      Title="SearchPage" Background="#F0F0F0">

    <Grid Background="#E5E5E5">
        <StackPanel x:Name="ResultsStackPanel" Grid.Row="2" Grid.Column="2" Background="#F5F5F5">
        </StackPanel>
    </Grid>
</Page>
